{
  "name": "kraken",
  "description": "Kraken Software is the project management tool for agile teams",
  "author": "Chams AGOUNI <agounichams1@gmail.com>",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "graphql": "^14.3.1",
    "graphql-request": "^1.8.2",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1"
  },
  "files": [
    "*.js",
    "build",
    "node_modules"
  ],
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "start": "nf start -p 5400",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "react-start": "BROWSER=none react-scripts start",
    "dist": "npm run build && build",
    "postinstall": "install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron-builder": "^20.41.0",
    "electron-packager": "^13.1.1"
  },
  "build": {
    "appId": "com.electron.electron-with-create-react-app",
    "files": [
      "*.js",
      "build",
      "node_modules"
    ],
    "win": {
      "icon": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
    },
    "directories": {
      "buildResources": "public"
    }
  }
}
